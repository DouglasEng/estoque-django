services:
  - type: web
    name: django1
    env: python
    buildCommand: "./build.sh"
    startCommand: "gunicorn django1.wsgi:application"
    envVars:
      - key: DJANGO_SETTINGS_MODULE
        value: django1.settings
      - key: SECRET_KEY
        generateValue: true
      - key: DEBUG
        value: False
      - key: ALLOWED_HOSTS
        value: django1.onrender.com
